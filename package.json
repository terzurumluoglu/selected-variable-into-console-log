{
  "name": "selected-variable-into-console-log",
  "displayName": "Selected Variable into Console Log",
  "description": "You can pass variable into console by using this extension",
  "version": "0.2.1",
  "engines": {
    "vscode": "^1.105.0"
  },
  "publisher": "toprak",
  "author": {
    "name": "terzurumluoglu"
  },
  "homepage": "https://marketplace.visualstudio.com/items?itemName=toprak.selected-variable-into-console-log",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/terzurumluoglu/selected-variable-into-console-log"
  },
  "icon": "images/icon.png",
  "categories": [
    "Programming Languages",
    "Debuggers",
    "Other"
  ],
  "keywords": [
    "JavaScript",
    "TypeScript",
    "Print",
    "Log"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "selected-variable-into-console-log.insertConsoleLog",
        "title": "Insert Console Log"
      }
    ],
    "keybindings": [
      {
        "command": "selected-variable-into-console-log.insertConsoleLog",
        "key": "ctrl+alt+l",
        "mac": "cmd+shift+l"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^13.1.1",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.105.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.36.0",
    "semantic-release": "^25.0.1",
    "typescript": "^5.9.3"
  }
}
